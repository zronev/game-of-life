{
  "name": "game-of-life",
  "scripts": {
    "dev-react": "parcel src/app/react/index.html --open",
    "dev-vanilla": "parcel src/app/vanilla/index.html --open",
    "build-react": "parcel build src/app/react/index.html",
    "build-vanilla": "parcel build src/app/vanilla/index.html",
    "test": "jest",
    "coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "pre-commit": "yarn type-check && yarn test && yarn lint"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
